<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androideatit">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidEatIt">
        <activity android:name=".OrderSattus"></activity>
        <activity android:name=".Cart" />
        <!-- Begin: BinhPT06 - xu ly cho back ve man hinh truoc do-->
        <activity android:name=".FoodDetail"
            android:parentActivityName=".FoodList">
        </activity>
        <activity android:name=".FoodList"
            android:parentActivityName=".Home">
        </activity>
        <!-- End: BinhPT06 - xu ly cho back ve man hinh truoc do-->
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.AndroidEatIt.NoActionBar" />
        <activity android:name=".SignUp" />
        <activity android:name=".SignIn" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>